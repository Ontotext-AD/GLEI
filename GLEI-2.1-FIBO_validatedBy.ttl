@base <http://data.ontotext.com/resource/glei/>.

<(LEI)/entry>
  leio:validation <(LEI)/id/register/(Registration/ValidationAuthority/ValidationAuthorityID)>;
  leio:validation <(LEI)/literal/validation-authority/($n)>.

<(LEI)/id/register/(Registration/ValidationAuthority/ValidationAuthorityID)>
  puml:label """
ValidationAuthorityID/[ValidationAuthorityID!='RA888888' and
 ValidationAuthorityID!='RA999999' and ValidationAuthorityEntityID]
""";
  leio:validatedBy <register/(ValidationAuthorityID)>;
  a fibo-be-corp-corp:RegistrationIdentifier;
  fibo-fnd-rel-rel:hasUniqueIdentifier "(ValidationAuthorityEntityID)".

<(LEI)/literal/validation-authority/($n)>
  puml:label """
ValidationAuthorityID/OtherValidationAuthorityID at $n
""";
  a fibo-be-corp-corp:RegistrationIdentifier;
  rdfs:comment ".";
  fibo-fnd-rel-rel:hasUniqueIdentifier "(ValidationAuthorityEntityID)".

